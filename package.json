{
  "name": "pascua",
  "version": "2.1.8",
  "description": "Calculate Easter date for a given year",
  "main": "dist/pascua.js",
  "scripts": {
    "format": "prettier . --write",
    "lint": "eslint src --fix",
    "test": "vitest run",
    "build": "tsup src/pascua.ts --dts --format cjs,esm --out-dir dist",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MauricioRobayo/pascua/issues"
  },
  "homepage": "https://github.com/MauricioRobayo/pascua#readme",
  "author": "Mauricio Robayo <rfmajo@gmail.com> (https://www.mauriciorobayo.com)",
  "keywords": [
    "easter",
    "easter date",
    "gregorian calendar",
    "holidays"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "semantic-release": "^24.0.0",
    "tsup": "^8.5.0",
    "typescript": "^4.6.4",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MauricioRobayo/pascua.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
